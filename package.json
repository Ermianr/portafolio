{
    "name": "portafolio",
    "version": "0.0.1",
    "devDependencies": {
        "@biomejs/biome": "~2.0.6",
        "@sveltejs/adapter-auto": "~6.0.1",
        "@sveltejs/kit": "~2.22.2",
        "@sveltejs/vite-plugin-svelte": "~5.1.0",
        "@tailwindcss/vite": "^4.1.11",
        "clsx": "~2.1.1",
        "svelte": "~5.35.2",
        "svelte-check": "~4.2.2",
        "tailwind-merge": "~3.3.1",
        "tailwind-variants": "~1.0.0",
        "tailwindcss": "~4.1.11",
        "tw-animate-css": "~1.3.5",
        "typescript": "~5.8.3",
        "vite": "~7.0.2"
    },
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "bunx biome check --write ./src/lib/components ./src/routes",
        "unsafe": "bunx biome check --write --unsafe ./src/lib/components/layout"
    },
    "type": "module",
    "dependencies": {
        "@lucide/svelte": "^0.525.0"
    }
}
